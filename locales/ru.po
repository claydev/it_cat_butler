#: plugins/notifications.lua:6
#, lua-format
msgid "%s [mentioned](%s) you in the group \"%s\""
msgstr "%s [упомянул](%s) тебя в группе «%s»"

#: plugins/notifications.lua:8
#, lua-format
msgid "%s mentioned you in the group \"%s\""
msgstr "%s упомянул тебя в группе «%s»"

#: plugins/notifications.lua:40
msgid "_The subscribe to your mentions has activated successfully_"
msgstr "_Подписка на твои упоминания успешно активирована_"

#: plugins/notifications.lua:42
msgid "_Your subscribe already activated_"
msgstr "_Подписка уже активирована_"

#: plugins/notifications.lua:44
msgid "Notifications will not come until you message me"
msgstr "Уведомления не будут приходить, пока ты не напишешь мне"

#: plugins/notifications.lua:55
msgid "_The subscribe has deactivated successfully_"
msgstr "_Подписка успешно деактивирована_"

#: plugins/notifications.lua:57
msgid "_Your subscribe already deactivated_"
msgstr "_Подписка уже деактивирована_"

#: plugins/notifications.lua:91
msgid ""
"To unsubscribe, answer me to unwanted notification with command `/unscribe`"
msgstr ""
"Чтобы отписаться, ответь мне командой `/unscribe` на нежелательное "
"уведомление"
